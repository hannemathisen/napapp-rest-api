RewriteEngine On # Turn on the rewriting engine
RewriteRule ^cars/?$ cars.php [NC,L]
RewriteRule ^cars/([0-9]+)/?$ cars.php?id=$1 [NC,L]

RewriteRule ^users/?$ users.php [NC,L]
RewriteRule ^users/([0-9]+)/?$ users.php?id=$1 [NC,L]

RewriteRule ^rides/?$ rides.php [NC,L]
RewriteRule ^rides/([0-9]+)/?$ rides.php?id=$1 [NC,L]
